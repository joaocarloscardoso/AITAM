<% plancatalog.forEach(function(item) { %>
    <tr id="<%= item.RowId %>Row" style="display: table-row;">
        <td>
            <%= item.Domain %>
            <input type="hidden" id="<%= item.RowId %>Domain" name="<%= item.RowId %>Domain" value="<%= item.DomainId %>"/>
            <input type="hidden" id="<%= item.RowId %>Plugin" name="<%= item.RowId %>Plugin" value="<%= item.PluginId %>"/>
        </td>
        <td>
            <%= item.Area %>
            <input type="hidden" id="<%= item.RowId %>Area" name="<%= item.RowId %>Area" value="<%= item.AreaId %>"/>
        </td>
        <td>
            <a href="/auditMatrixes/planMatrix?plugin=<%= item.PluginId %>&domain=<%= item.DomainId %>area=<%= item.AreaId %>&issue=<%= item.IssueId %>"><%= item.Issue %></a>
            <input type="hidden" id="<%= item.RowId %>Issue" name="<%= item.RowId %>Issue" value="<%= item.IssueId %>"/>
        </td>
        <td>
            <select class="custom-select" name="<%= item.RowId %>Risk" id="<%= item.RowId %>Risk" size="1" autofocus onchange="showRisk(this, '<%= item.RowId %>')">
                <% if (item.Risk == "3") { %>
                    <option value="3" selected>High</option>
                    <option value="2">Medium</option>
                    <option value="1">Low</option>
                <% } else if (item.Risk == "2") { %>
                    <option value="3">High</option>
                    <option value="2" selected>Medium</option>
                    <option value="1">Low</option>
                <% } else if (item.Risk == "1") { %>
                    <option value="3">High</option>
                    <option value="2">Medium</option>
                    <option value="1" selected>Low</option>
                <% } %>                
            </select>
            <% if (item.Risk == "3") { %>
                <img id="rhigh<%= item.RowId %>" src="../images/high-16.png" alt="" style="vertical-align: text-bottom; display: inline-block;"/>
                <img id="rmedium<%= item.RowId %>" src="../images/medium-16.png" alt="" style="vertical-align: text-bottom; display: none;"/>
            <% } else if (item.Risk == "2") { %>
                <img id="rhigh<%= item.RowId %>" src="../images/high-16.png" alt="" style="vertical-align: text-bottom; display: none;"/>
                <img id="rmedium<%= item.RowId %>" src="../images/medium-16.png" alt="" style="vertical-align: text-bottom; display: inline-bloc;"/>
            <% } else if (item.Risk == "1") { %>
                <img id="rhigh<%= item.RowId %>" src="../images/high-16.png" alt="" style="vertical-align: text-bottom; display: none;"/>
                <img id="rmedium<%= item.RowId %>" src="../images/medium-16.png" alt="" style="vertical-align: text-bottom; display: none;"/>
            <% } %>                
        </td>
        <td>
            <select class="custom-select" name="<%= item.RowId %>Include" id="<%= item.RowId %>Include" size="1" autofocus onchange="showInclude(this, '<%= item.RowId %>Image')">
                <% if (item.Selected == "Yes") { %>
                    <option value="Yes" selected>Yes</option>
                    <option value="No">No</option>
                <% } else { %>
                    <option value="Yes">Yes</option>
                    <option value="No" selected>No</option>
                <% } %>                
            </select>
            <% if (item.Selected == "Yes") { %>
                <img id="<%= item.RowId %>Image" src="../images/include-16b.png" alt="" style="vertical-align: text-bottom; display: inline-block;"/>
            <% } else { %>
                <img id="<%= item.RowId %>Image" src="../images/include-16b.png" alt="" style="vertical-align: text-bottom; display: none;"/>
            <% } %>                
        </td>
        <td>
            <input type="text" id="<%= item.RowId %>Remarks" name="<%= item.RowId %>Remarks" value="<%= item.Remarks %>"/>
        </td>
    </tr>
<% }) %>
    